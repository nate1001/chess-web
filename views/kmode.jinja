{% extends "base.jinja" %}
{% block title %}{{ title }}{% endblock %}
{% block title2 %}{{ title }} {% endblock %}
{% block content %}
    {% for centroid in rows%}

        <div class='inblock'>
          {{ centroid.centroid.to_svg(250)}}
          <div class='inblock'>
            <h4 ><a href=/{{folder}}/{{centroid.kclass}}>centroid {{centroid.kclass}}</a></h4>
            <table>
                    <tr><td>n</td><td class='number'> {{centroid.count}}</td></tr>
                    <tr><td>hamming avg</td><td class='number'> {{centroid.avg}}</td></tr>
                    <tr><td>hamming se</td><td class='number'> {{centroid.stddev}}</td></tr>
                    <tr><td>win %</td><td class='number'> {{centroid.win}}</td></tr>
                    <tr><td>avg rating</td><td class='number'> {{centroid.rating}}</td></tr>
            </table>
          </div>
        </div>
    {% endfor %}

{% endblock %}

